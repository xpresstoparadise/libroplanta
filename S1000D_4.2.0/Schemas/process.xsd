        <ref>7.6.1.4</ref>
      </xs:documentation>
      <xs:documentation xml:lang="ru">
        <title>Действительное значение</title>
        <description>Действительное значение</description>
        <ref>7.6.1.4</ref>
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="integerValue" type="xs:string">
