      <xs:documentation xml:lang="ru">
        <title>Логическое значение</title>
        <description>Логическое значение</description>
        <ref>7.6.1.4</ref>
      </xs:documentation>
      <xs:documentation xml:lang="en">
        <title>Expressions</title>
