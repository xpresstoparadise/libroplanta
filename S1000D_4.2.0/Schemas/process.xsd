      <xs:group maxOccurs="unbounded" minOccurs="0" ref="figureElemGroup" />
      <xs:choice>
        <xs:sequence>
          <xs:sequence maxOccurs="unbounded">
            <xs:element minOccurs="0" ref="note" />
