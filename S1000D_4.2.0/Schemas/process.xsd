  <xs:attribute default="nominal" name="quantityGroupType" type="quantityGroupTypeAttType">
    <xs:annotation>
      <xs:documentation xml:lang="ru">
        <title>Тип количественной группы</title>
        <description>Тип количественной группы</description>
        <ref>3.9.5.2.1.10</ref>
      </xs:documentation>
      <xs:documentation xml:lang="en">
        <title>Quantity group</title>
        <description>quantityGroupType, a classification of the quantity group. The attribute can have one of the following values  - "nominal", the usual value of the quantity  - "minimum", the smallest value of the quantity  - "maximum", the largest value of the quantity</description>
        <ref>3.9.5.2.7</ref>
      </xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:simpleType name="quantityGroupTypeAttType">
