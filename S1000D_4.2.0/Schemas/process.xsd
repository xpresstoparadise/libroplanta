            <ref>3.9.6.1</ref>
          </xs:documentation>
          <xs:documentation xml:lang="en">
            <title>Bullet</title>
            <description>Bullet (outline - ISOpub: bull)</description>
            <ref>3.9.6.1</ref>
          </xs:documentation>
        </xs:annotation>
