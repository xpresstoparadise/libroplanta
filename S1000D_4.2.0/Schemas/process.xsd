    <xs:annotation>
      <xs:documentation xml:lang="ru">
        <title>Подгруппа описаний автоматов защиты</title>
        <description>Элемент circuitBreakerDescrSubGroup содержит один или несколько вложенных перечней автоматов защиты.</description>
        <ref>3.9.5.2.1.9</ref>
      </xs:documentation>
      <xs:documentation xml:lang="en">
        <title>Circuit breaker description subgroup</title>
        <description>The element &lt;circuitBreakerDescrSubGroup&gt; contains one or more sub circuit breaker lists.</description>
        <ref>3.9.5.2.1.9</ref>
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="circuitBreakerDescrSubGroupElemType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" ref="functionalItemRef" />
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="circuitBreakerDescr" />
      </xs:sequence>
    </xs:sequence>
    <xs:attribute ref="circuitBreakerAction" />
    <xs:attribute ref="checkSum" />
    <xs:attribute ref="applicRefId" />
