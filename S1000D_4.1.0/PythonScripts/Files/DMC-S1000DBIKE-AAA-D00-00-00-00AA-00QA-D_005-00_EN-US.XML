		<referencedApplicGroup>
			<!-- The values here are used for dependency checking, not for filtering. It does not indicate the current state of the airplane. The state of the airplane is provided by the IETM. -->
			<applic id="A-1" reasonForUpdateRefIds="rfu-003">
				<!-- This will evaluate to true if SB 1 has been applied to the airplane -->
				<displayText>
					<simplePara>SB 1</simplePara>
				</displayText>
				<assert applicPropertyType="condition" applicPropertyIdent="SB1" applicPropertyValues="POST-001~POST-999"/>
			</applic>
			<applic id="A-2">
				<displayText>
					<simplePara>POST SB 1 AND PRE SB 9</simplePara>
				</displayText>
				<evaluate andOr="and">
					<assert applicPropertyType="condition" applicPropertyIdent="SB1" applicPropertyValues="POST-001~POST-999"/>
					<assert applicPropertyType="condition" applicPropertyIdent="SB9" applicPropertyValues="PRE"/>
				</evaluate>
			</applic>
		</referencedApplicGroup>
